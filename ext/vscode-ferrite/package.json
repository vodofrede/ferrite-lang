{
    "name": "vscode-ferrite",
    "displayName": "vscode-ferrite",
    "description": "Language support for the Ferrite programming language.",
    "publisher": "vodofrede",
    "version": "0.1.0",
    "repository": {
        "url": "https://github.com/vodofrede/ferrite-lang"
    },
    "engines": {
        "vscode": "^1.89.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ferrite",
                "aliases": [
                    "Ferrite",
                    "ferrite"
                ],
                "extensions": [
                    ".fr"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ferrite",
                "scopeName": "source.ferrite",
                "path": "./syntaxes/ferrite.tmLanguage.json"
            }
        ]
    }
}
